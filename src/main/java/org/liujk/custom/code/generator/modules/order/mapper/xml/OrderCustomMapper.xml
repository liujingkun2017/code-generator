<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.liujk.custom.code.generator.modules.order.mapper.OrderCustomMapper">

	<delete id="deleteByMainId" parameterType="java.lang.String">
		DELETE 
		FROM  order_customer 
		WHERE
			 order_id = #{id} 		
	</delete>
	
	<select id="selectByMainId" parameterType="java.lang.String" resultType="org.liujk.custom.code.generator.modules.order.entity.OrderCustom">
		SELECT * 
		FROM  order_customer
		WHERE
			 order_id = #{id} 		
	</select>
</mapper>
